<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_page"
    app:startDestination="@id/PageMainFragment">

    <fragment
        android:id="@+id/PageMainFragment"
        android:name="com.example.cantinhodecoracao.Fragments.PageFragments.PageFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_page_main">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/ClientFragment" />
    </fragment>

    <fragment
        android:id="@+id/ClientFragment"
        android:name="com.example.cantinhodecoracao.Fragments.PageFragments.ClientFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_client">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/PageMainFragment" />
    </fragment>
</navigation>